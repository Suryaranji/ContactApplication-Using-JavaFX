<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.contacts.AddContactsController"
            prefHeight="300.0" prefWidth="400">
    <headerText>
        Add Contact
    </headerText>
    <content>
        <GridPane hgap="10" vgap="10" alignment="CENTER">
            <Label text="First Name" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="fname" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Last Name" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <TextField fx:id="lname" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <Label text="Phone Number" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <TextField fx:id="number" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
            <Label text="Notes" GridPane.rowIndex="3" GridPane.columnIndex="0"/>
            <TextField fx:id="notes" GridPane.rowIndex="3" GridPane.columnIndex="1"/>

        </GridPane>
    </content>

</DialogPane>
